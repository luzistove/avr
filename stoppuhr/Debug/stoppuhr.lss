                                 .list
                                 
                                 ;.def label = r26	; define r26 as the count/pause label, every time 
                                 					; when the start/stop button is pressed, r26 will be 
                                 					; judged. When r26 is 0x00, then it means the status of
                                 					; pause; when it is 0xFF, it means the status of counting
                                 
                                 .DSEG
                                 .ORG 0x0060				; In SRAM at the address of 0x0060, we reserve 4 bytes for storing the 
000060                           table_stelle: .BYTE 4	; code of chip selection, to select which display should be on
                                 .ORG 0x0070				; Similarly, at the address of 0x0070, 10 bytes for the code of segment
000070                           table_nummer: .BYTE 10	; selection, to display number
                                 
                                 
                                 ;.equ F_quarz = 4000000
                                 ;.equ baud = 9600
                                 ;.equ UBRR_VAL = ((F_quarz+baud*8)/(baud*16)-1)
                                 
                                 
                                 .CSEG
                                 .org 0x0000
000000 c016                      		rjmp main	
                                 
                                 .org 0x002		; INT0, reset
000002 c053                      		rjmp display_reset_init
                                 
                                 .org 0x004		; INT1, start/stop
000004 c06c                      		rjmp mode_select_avr
                                 
                                 .org 0x0010		; Interrupt vector of timer1 overflow
000010 c0e9                      		rjmp timer1_overflow
                                 
                                 .org 0x0016
000016 c065                      		rjmp mode_select_pc
                                 
                                 ;================================================
                                 ;            Stack pointer initialize
                                 ;================================================
                                 
                                 
                                 main:
                                 		;Stackpointer initialize
000017 e004                      		ldi r16, HIGH(RAMEND)
000018 bf0e                      		out SPH, r16
000019 e50f                      		ldi r16, LOW(RAMEND)
00001a bf0d                      		out SPL, r16
                                 		
00001b e0f0                      		ldi ZH, HIGH(table_stelle)		; Use Y and Z pointer to get the addresses of the lookup tables
00001c e6e0                      		ldi ZL, LOW(table_stelle)
                                 
                                 
00001d e0d0                      		ldi YH, HIGH(table_nummer)
00001e e7c0                      		ldi YL, LOW(table_nummer)
                                 
                                 ;================================================
                                 ;               Write lookup table
                                 ;================================================
00001f ef0a                      		ldi r16, 0xFA
000020 8300                      		std Z+0, r16
000021 ef09                      		ldi r16, 0xF9
000022 8301                      		std Z+1, r16
000023 ef03                      		ldi r16, 0xF3
000024 8302                      		std Z+2, r16
000025 ee0b                      		ldi r16, 0xEB
000026 8303                      		std z+3, r16	
                                 		
000027 ef0d                      		ldi r16, 0xFD
000028 8308                      		std Y+0, r16
000029 e109                      		ldi r16, 0x19
00002a 8309                      		std Y+1, r16
00002b ed07                      		ldi r16, 0xD7
00002c 830a                      		std Y+2, r16
00002d e50f                      		ldi r16, 0x5F
00002e 830b                      		std Y+3, r16
00002f e20b                      		ldi r16, 0x2B
000030 830c                      		std Y+4, r16
000031 e70e                      		ldi r16, 0x7E
000032 830d                      		std Y+5, r16
000033 ef0e                      		ldi r16, 0xFE
000034 830e                      		std Y+6, r16
000035 e10d                      		ldi r16, 0x1D
000036 830f                      		std Y+7, r16
000037 ef0f                      		ldi r16, 0xFF
000038 8708                      		std Y+8, r16
000039 e70f                      		ldi r16, 0x7F
00003a 8709                      		std Y+9, r16
                                 
                                 ;================================================
                                 ;               Output initialize
                                 ;================================================
                                 output_init:
                                 
00003b 2700                      		clr r16
                                 
00003c ef0f                      		ldi r16, 0xFF	; Port A and C are used as output ports, to light the display
00003d bb0a                      		out DDRA, r16
00003e bb04                      		out DDRC, r16
                                 
00003f 2700                      		clr r16			; The external interrupt pins in port D are also set as output
000040 e00e                      		ldi r16, 0x0E
000041 bb01                      		out DDRD, r16
000042 9a92                      		sbi PORTD, 2
000043 9a93                      		sbi PORTD, 3
                                 
                                 
